{
  "endpoints": {
    "serverUrl": "http://localhost:3000/api",
    "categories": {
      "getPaginated": "categories/get-paginated?page={pageNo}&limit={pageSize}&name={name}",
      "create": "categories",
      "update": "categories/{id}",
      "view": "categories/{id}",
      "delete": "categories/{id}"
    },
    "brands": {
      "create": "brands",
      "update": "brands/{id}",
      "view": "brands/{id}",
      "delete": "brands/{id}"
    },
    "products": {
      "getPaginated": "products/get-paginated?page={pageNo}&limit={pageSize}&name={name}",
      "create": "products",
      "update": "products/{id}",
      "view": "products/{id}",
      "delete": "products/{id}",
      "isDeletable": "products/{id}/is-deletable"
    }
  }
}